{
  "countries": [
    {
      "code": "IN",
      "name": "India",
      "rules": [
        {
          "id": "in_sender_ends_s",
          "name": "Senders ending with -S",
          "description": "Standard bank sender format in India",
          "pattern": ".*-S$",
          "enabled": true
        },
        {
          "id": "in_contains_bank",
          "name": "Contains BANK keyword",
          "description": "Senders containing BANK keyword",
          "pattern": "BANK",
          "enabled": true
        },
        {
          "id": "in_contains_upi",
          "name": "Contains UPI keyword",
          "description": "UPI-related senders",
          "pattern": "UPI",
          "enabled": true
        },
        {
          "id": "in_contains_transaction",
          "name": "Contains TRANSACTION keyword",
          "description": "Generic transaction senders",
          "pattern": "TRANSACTION",
          "enabled": true
        },
        {
          "id": "in_bank_codes_hdfc",
          "name": "HDFC Bank senders",
          "description": "HDFC Bank transaction messages",
          "pattern": "^(HDFC|VK-HDFC|HDFCBK).*",
          "enabled": true
        },
        {
          "id": "in_bank_codes_icici",
          "name": "ICICI Bank senders",
          "description": "ICICI Bank transaction messages",
          "pattern": "^(ICICI|ICICIB|VM-ICIC).*",
          "enabled": true
        },
        {
          "id": "in_bank_codes_sbi",
          "name": "SBI senders",
          "description": "State Bank of India messages",
          "pattern": "^(SBI|SBICRD|ATMSBI).*",
          "enabled": true
        }
      ]
    }
  ]
}
